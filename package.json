  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "http-server": "^0.9.0",
    "jest": "^19.0.2",
    "prcl": "^0.4.0",
    "webpack": "^2.3.3"
